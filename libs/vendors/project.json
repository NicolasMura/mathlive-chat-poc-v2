{
  "projectType": "library",
  "sourceRoot": "libs/vendors/src",
  "prefix": "mathlive-chat-poc",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/vendors"
      ],
      "options": {
        "jestConfig": "libs/vendors/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/vendors/**/*.ts",
          "libs/vendors/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
